
public class Dog {
   private int dogSpaceNumber;
   private int dogWeight;
   private boolean grooming;

   public Dog() {
      this.dogSpaceNumber = -1;
      this.dogWeight = -1;
      this.grooming = false;
   }

   // Grooming
   public boolean getGrooming() {
      return this.grooming;
   }

   public void setGrooming(boolean groom) {
      this.grooming = groom;
   }

   // Weight
   public int getDogWeight() {
      return this.dogWeight;
   }

   public void setDogWeight(int weight) {
      this.dogWeight = weight;
   }

   // Space ID
   public int getDogSpaceNumber() {
      return this.dogSpaceNumber;
   }

   public void setDogSpaceNumber(int spaceID) {
      this.dogSpaceNumber = spaceID;
   }

   // Main
   public static void main(String[] args) {
      Dog buddy = new Dog();
      buddy.setName("Buddy");
      buddy.setAge(3);
      buddy.setDogWeight(50);
      buddy.setGrooming(true);
      buddy.setDogSpaceNumber(7);
      System.out.println(buddy.getName() + " is " + buddy.getAge() + " years old, weighs " + buddy.getDogWeight() + " pounds, grooming: " + buddy.getGrooming() + ", space number: " + buddy.getDogSpaceNumber());
   }
}
